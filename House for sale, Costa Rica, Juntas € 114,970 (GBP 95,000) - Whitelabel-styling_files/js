        Browser.IsPhone = function () {
            return Modernizr.mq('only screen and (max-width: 767px)');
        };
        Browser.IsTablet = function () {
            return Modernizr.mq('only screen and (min-width : 768px) and (max-width : 1024px)');
        };
        Browser.IsMobile = function () {
            return Browser.IsPhone() || Browser.IsTablet();
        };
        Browser.IsDesktop = function () {
            return Modernizr.mq('only screen and (min-width: 1025px)');
        };
        //static ClickEventName = 'click';
        Browser.ClickEventName = Browser.mobileCheck() ? 'touchstart' : 'click';
        Browser.MouseUpEventName = Browser.mobileCheck() ? 'touchend' : 'mouseup';
        Browser.ChangeEventName = 'change';
        Browser.Click = 'click';
        return Browser;
    }());
    System.Browser = Browser;
    function getLanguage() {
        var language = $('#menu-immobox').attr('data-languageid');
        if (language == undefined) {
            var href = window.location.pathname;
            language = href.substr(href.indexOf('/') + 1, 2);
        }
        return language;
    }
    System.getLanguage = getLanguage;
    var UserAuthenticationType = (function () {
        function UserAuthenticationType() {
        }
        UserAuthenticationType.Guest = "Guest";
        UserAuthenticationType.Light = "Light";
        UserAuthenticationType.FullAuthenticated = "Full";
        UserAuthenticationType.Unknown = "Unknown";
        return UserAuthenticationType;
    }());
    System.UserAuthenticationType = UserAuthenticationType;
    function getUserAuthenticationType() {
        var userAuthenticationType = UserAuthenticationType.Unknown;
        $.ajax({
            method: 'POST',
            // It is important to have async: false here
            async: false,
            url: '/' + getLanguage() + '/Account/GetUserAuthenticationType',
            success: function (result) {
                try {
                    userAuthenticationType = result.userAuthenticationType;
                }
                catch (ex) {
                    userAuthenticationType = UserAuthenticationType.Unknown;
                }
            }
        });
        return userAuthenticationType;
    }
    System.getUserAuthenticationType = getUserAuthenticationType;
    var Form = (function () {
        function Form() {
        }
        Form.Initialize = function ($form) {
            // Validation
            $.validator.unobtrusive.parseDynamicContent($form);
            Form.InitializePlaceholders($form);
        };
        Form.InitializePlaceholders = function ($form) {
            // Placeholders for old browsers
            if (!Modernizr.placeholder && $form.find('.polyfill-placeholder').length == 0) {
                $form.find('input[placeholder]').placeholder({ inheritStyle: false });
            }
        };
        return Form;
    }());
    System.Form = Form;
    var Module = (function () {
        function Module() {
        }
        Module.IsMapMode = function () {
            return $('#layout').hasClass('__search-engine-map-mode') || $('body').hasClass('__map-mode');
        };
        return Module;
    }());
