        };
        GoogleTagManagerTracker.prototype.TrackEventCheck = function (trackerName, eventName, params) {
            if (!_super.prototype.TrackEventCheck.call(this, trackerName, eventName, params)) {
                return false;
            }
            return true;
        };
        return GoogleTagManagerTracker;
    }(GenericTracker));
    GenericTracking.GoogleTagManagerTracker = GoogleTagManagerTracker;
    var ListHubTracker = (function (_super) {
        __extends(ListHubTracker, _super);
        function ListHubTracker(trackingName, trackingItems) {
            _super.call(this, trackingItems);
            this.autoInitialize = true;
            this.ListHubEventNames = {};
            this.Setup(trackingName, trackingItems);
            //todo use default event list array in properties
            this.SubscribeToEvent(GenericTracking.TrackingEvent[GenericTracking.TrackingEvent.ViewCampaign]);
            this.SubscribeToEvent(GenericTracking.TrackingEvent[GenericTracking.TrackingEvent.SearchResult]);
            this.SubscribeToEvent(GenericTracking.TrackingEvent[GenericTracking.TrackingEvent.CallOfficePhone]);
            this.SubscribeToEvent(GenericTracking.TrackingEvent[GenericTracking.TrackingEvent.CallAgencyPhone]);
        }
        ListHubTracker.prototype.Initialize = function () {
            var trackingItem = this.TrackingItems[0];
            this.ListHubEventNames[GenericTracking.TrackingEvent[GenericTracking.TrackingEvent.ViewCampaign]] = "DETAIL_PAGE_VIEWED";
            this.ListHubEventNames[GenericTracking.TrackingEvent[GenericTracking.TrackingEvent.CallOfficePhone]] = "OFFICE_PHONE_CALL";
            this.ListHubEventNames[GenericTracking.TrackingEvent[GenericTracking.TrackingEvent.CallAgencyPhone]] = "AGENT_PHONE_CALL";
            this.ListHubEventNames[GenericTracking.TrackingEvent[GenericTracking.TrackingEvent.SearchResult]] = "SEARCH_DISPLAY";
            console.log("Init with code: " + trackingItem.Code);
            (function (l, i, s, t, h, u, b) {
                l['ListHubAnalyticsObject'] = h;
                l[h] = l[h] || function () {
                    (l[h].q = l[h].q || []).push(arguments);
                }, l[h].d = new Date().valueOf();
                u = i.createElement(s),
                    b = i.getElementsByTagName(s)[0];
                u.async = 1;
                u.src = t;
                b.parentNode.insertBefore(u, b);
            })(window, document, 'script', '//tracking.listhub.net/la.min.js', 'lh', '1', '1');
            lh('init', { provider: trackingItem.Code, test: trackingItem.Label === "test" ? true : false });
        };
        ListHubTracker.prototype.TrackEvent = function (trackerName, eventName, params) {
            var references = [];
            if (params != null) {
                for (var i = 0; i < params.OrganizationId.length; i++) {
                    if (params.OrganizationId[i].toLowerCase() === this.TrackingItems[0].Value.toLowerCase()) {
                        references.push({ lkey: params.PropertyRefNumber[i].replace("ListHubFang-", "") });
                    }
                }
            }
            if (references.length > 0) {
                lh('submit', this.ListHubEventNames[eventName], references);
                console.log(trackerName + " TrackEvent for " + eventName + " with tracking data: " + JSON.stringify(references));
            }
            return true;
        };
        ListHubTracker.prototype.TrackEventCheck = function (trackerName, eventName, params) {
            if (!_super.prototype.TrackEventCheck.call(this, trackerName, eventName, params)) {
                return false;
            }
            if (params != null && params.OrganizationId && params.PropertyRefNumber) {
                for (var i = 0; i < params.OrganizationId.length; i++) {
                    if (params.OrganizationId[i].toLowerCase() === this.TrackingItems[0].Value.toLowerCase()) {
                        return true;
                    }
                }
            }
            ;
            return false;
        };
        return ListHubTracker;
    }(GenericTracker));
    GenericTracking.ListHubTracker = ListHubTracker;
    var NetMetrixAudit = (function (_super) {
