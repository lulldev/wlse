 /// End file: warning.js


/// Start file: detail_map.js

//Map              
var detailmap = null;
var detailmapLat = null;
var detailmapLng = null;

//Item
var imageItem = null;

//Interest markers
var interestsBuffer = new Array();
var interestsImage= new Array();
var interestRadius = null;

////Geocoder
//var geocoder = null;
//var geoFinded = null;

//Zoom Lvl
var detailmapZoomMin = null;
var detailmapZoomMax = null;
var detailmapInitialZoom = null;

//House marker
var houseMarker = null;

// geocode
var geocodingConfig = null;

var ZOOM_INITIAL = 1;
var ZOOM_MAX = 2;
var ZOOM_MIN = 3;
var ZOOMLEVEL_DEFAULT = 1;
var ZOOMLEVEL_LOCATION = 2;
var ZOOMLEVEL_COUNTRY = 3;

//Process
function InitializeMapDetail(mapContainer) {
	//Init Map
	detailmapInitialZoom = GetMapDetailZoom(ZOOM_INITIAL, ZOOMLEVEL_DEFAULT);
	detailmapLat = $('#HiddenCenterLatitude') ? $('#HiddenCenterLatitude')[0].value : $('#InterfaceMapCenterLatitude')[0].value;
	detailmapLng = $('#HiddenCenterLongitude') ? $('#HiddenCenterLongitude')[0].value : $('#InterfaceMapCenterLongitude')[0].value;
	detailmapZoomMin = GetMapDetailZoom(ZOOM_MIN, ZOOMLEVEL_DEFAULT);
	detailmapZoomMax = GetMapDetailZoom(ZOOM_MAX, ZOOMLEVEL_DEFAULT);

	var viewPoint;
	if (detailmapLat == '' || detailmapLng == '' || detailmapLat == null || detailmapLng == null) {
		if (geocodingConfig && geocodingConfig.address && geocodingConfig.location && geocodingConfig.location.PostCode) {
			$.ajax({
				type: "GET",
				url: geocodingConfig.geocodingUrl,
				data: { address: escape(geocodingConfig.address), zip: geocodingConfig.location.PostCode },
				contentType: "application/json; charset=utf-8",
				dataType: "json",
				async: false,
				success: function (pResult) {
					if (pResult && pResult.Latitude && pResult.Longitude) {
						detailmapLat = pResult.Latitude;
						detailmapLng = pResult.Longitude;
						detailmapInitialZoom = GetMapDetailZoom(ZOOM_INITIAL, ZOOMLEVEL_DEFAULT);
						detailmapZoomMin = GetMapDetailZoom(ZOOM_MIN, ZOOMLEVEL_DEFAULT);
						detailmapZoomMax = GetMapDetailZoom(ZOOM_MAX, ZOOMLEVEL_DEFAULT);
					}
					else {
						if (geocodingConfig != null && geocodingConfig.location != null && geocodingConfig.location.Latitude && geocodingConfig.location.Longitude) {
							detailmapLat = geocodingConfig.location.Latitude;
							detailmapLng = geocodingConfig.location.Longitude;
							detailmapInitialZoom = GetMapDetailZoom(ZOOM_INITIAL, ZOOMLEVEL_LOCATION);
							detailmapZoomMin = GetMapDetailZoom(ZOOM_MIN, ZOOMLEVEL_LOCATION);
							detailmapZoomMax = GetMapDetailZoom(ZOOM_MAX, ZOOMLEVEL_LOCATION);
						}
						else {
							detailmapLat = null;
							detailmapLng = null;
						}
					}
				},
				error: function (pResult) {
					//onWCFFailure(pResult);
				}
			});
		}
		else if (geocodingConfig != null && geocodingConfig.location != null && geocodingConfig.location.Latitude && geocodingConfig.location.Longitude) {
			detailmapLat = geocodingConfig.location.Latitude;
			detailmapLng = geocodingConfig.location.Longitude;
			detailmapInitialZoom = GetMapDetailZoom(ZOOM_INITIAL, ZOOMLEVEL_LOCATION);
			detailmapZoomMin = GetMapDetailZoom(ZOOM_MIN, ZOOMLEVEL_LOCATION);
			detailmapZoomMax = GetMapDetailZoom(ZOOM_MAX, ZOOMLEVEL_LOCATION);
		}
		else {
			detailmapLat = null;
			detailmapLng = null;
		}
	}

	if (detailmapLat == null || detailmapLng == null) {
		if (geocodingConfig != null && geocodingConfig.country && geocodingConfig.country.Latitude && geocodingConfig.country.Longitude) {
			detailmapLat = geocodingConfig.country.Latitude;
			detailmapLng = geocodingConfig.country.Longitude;
			detailmapInitialZoom = GetMapDetailZoom(ZOOM_INITIAL, ZOOMLEVEL_COUNTRY);
			detailmapZoomMin = GetMapDetailZoom(ZOOM_MIN, ZOOMLEVEL_COUNTRY);
			detailmapZoomMax = GetMapDetailZoom(ZOOM_MAX, ZOOMLEVEL_COUNTRY);
		}
		else {
			return false;
		}
	}

	var reg = new RegExp("(,)", "g");
	if (typeof detailmapLat == "string") {
		detailmapLat = detailmapLat.replace(reg, ".");
	}
	if (typeof detailmapLng == "string") {
		detailmapLng = detailmapLng.replace(reg, ".");
	}
	viewPoint = new google.maps.LatLng(detailmapLat, detailmapLng);


	var detailmapOptions = {
		disableDoubleClickZoom: true,
		mapTypeControl: true,
		navigationControl: true,
		scaleControl: true,
		scrollwheel: false,
		draggable: true,
		center: viewPoint,
		zoom: detailmapInitialZoom,
		mapTypeId: google.maps.MapTypeId.ROADMAP,
		streetViewControl: false,
		mapTypeControlOptions: { mapTypeIds: [google.maps.MapTypeId.ROADMAP, google.maps.MapTypeId.SATELLITE] }
	};

    detailmap = new google.maps.Map($('#' + mapContainer)[0], detailmapOptions);

    var houseMarkerOptions = {
        position: viewPoint,
        map: detailmap,
        clickable: false,
        draggable: false
    }
    if (!$.emptyString(imageItem)) {
        $.extend(true, houseMarkerOptions, { icon: GetIcon(imageItem) });
    }
    houseMarker = new google.maps.Marker(houseMarkerOptions);
    houseMarker.setMap(detailmap);

    google.maps.event.addListener(detailmap, "tilesloaded", function () {
        google.maps.event.trigger(detailmap, 'resize');
    }); 
        
    //Init Map Zoom Resolution
    document.cookie = "googleScriptLoaded=1; path=/";

    google.maps.event.addListenerOnce(detailmap, 'idle', function () {
    	google.maps.event.addListener(detailmap, 'zoom_changed', function () {
    		if (detailmap.getZoom() > detailmapZoomMax) {
    			detailmap.setZoom(parseInt(detailmapZoomMax));
    			return;
    		}
    		if (detailmap.getZoom() < detailmapZoomMin) {
    			this.setZoom(parseInt(detailmapZoomMin));
    			return;
    		}
    	});
    });
    return true;
}

function MapToolsCheckClick(p_obj) {
    //Draw interest points
    if(p_obj.checked && p_obj.attributes['typeid'] != undefined) doGetInterestByTypeFromCircle(p_obj.attributes['typeid'].nodeValue);
    //Clear interest points
    if(!p_obj.checked && p_obj.attributes['typeid'] != undefined) ClearMarkers(p_obj.attributes['typeid'].nodeValue);
}

function doGetInterestByTypeFromCircle(p_typeID) {
    //Get location JSON from WCF   
    var l_bounds = detailmap.getBounds();

    $.ajax({
    	type: "GET",
    	url: "/GIS/GetInterestByTypeFromCircle/",
    	data: { mapLng: detailmapLng, mapLat: detailmapLat, interestRadius: interestRadius, p_typeID: p_typeID },
    	contentType: "application/json; charset=utf-8",
    	dataType: "json",
    	success: function(pResult) {
	    	onGetInterestByTypeFromCircleSuccess(pResult);
    	},
    	error: function(pResult) {
    		onWCFFailure(pResult);
    	}
    });
}

function onGetInterestByTypeFromCircleSuccess(p_result) {
	if (p_result != null) {
        var jsonResult = p_result;
        DrawMarkers(jsonResult);
    }
}

function DrawMarkers(p_jsonMarkers) {
    //Get type interest
    var typeID = null;
	if (p_jsonMarkers != null && p_jsonMarkers != 0) {
        typeID = p_jsonMarkers[0].InterestTypeId;
        
        //Browse buffer and draw markers
		for (var i in p_jsonMarkers) {
            var tempBuffer = new Array();
			var center = new google.maps.LatLng(p_jsonMarkers[i].Latitude, p_jsonMarkers[i].Longitude);
			var marker = new google.maps.Marker({
				position: center,
				map: detailmap,
				icon: GetIcon(interestsImage[typeID]),
				clickable: false,
				draggable: false
			});
            tempBuffer.push(typeID,marker);
            interestsBuffer.push(tempBuffer);
        }
    }
}

function ClearMarkers(p_typeID) {
    //Browse collection and clear if need
	for (var i in interestsBuffer) {
		if (interestsBuffer[i][0] == p_typeID) {
			interestsBuffer[i][1].setMap(null);
			interestsBuffer[i][1] = null;
		}
    }
}
            
function GetIcon(p_imageUrl) {
    //Init icon size and image 
	var baseIcon = new google.maps.MarkerImage(p_imageUrl,
	// This marker is 23 pixels wide by 30 pixels tall.
		new google.maps.Size(30, 30),
	// The origin for this image is 0,0.
		new google.maps.Point(0, 0),
	// The anchor for this image is its center at 15,15.
		new google.maps.Point(15, 15));
	return baseIcon;
}

function InitMapConfig(p_jsonConfig, p_geocodingConfig) {
	if ($.isPlainObject(p_jsonConfig) && $.isPlainObject(p_geocodingConfig)) {

		if (p_jsonConfig.MapItemLayout != undefined) {
			imageItem = p_jsonConfig.MapItemLayout.MapItemImage != undefined && p_jsonConfig.MapItemLayout.MapItemImage.Image != undefined ? p_jsonConfig.MapItemLayout.MapItemImage.Image : "";

			if (p_jsonConfig.MapItemLayout.MapInterests != undefined) {
				var mapInterests = p_jsonConfig.MapItemLayout.MapInterests;
				interestRadius = mapInterests.Radius != undefined ? mapInterests.Radius : 0;
				for (var interest in mapInterests.MapInterest) {
					if (mapInterests.MapInterest[interest].hasOwnProperty("ImageMap")) {
						interestsImage[mapInterests.MapInterest[interest].TypeID] = mapInterests.MapInterest[interest].ImageMap;
					}
				}
			}
		}

		geocodingConfig = p_geocodingConfig;
	}
}

function GetMapDetailZoom(zoomType, zoomLevel) {
	var zType;
	switch (zoomType) {
		case ZOOM_INITIAL:
			zType = "InitialZoom";
			break;
		case ZOOM_MAX:
			zType = "ZoomMax";
			break;
		case ZOOM_MIN:
			zType = "ZoomMin";
			break;
		default:
			zType = "InitialZoom";
			break;
	}

	var zLevel;
	switch (zoomLevel) {
		case ZOOMLEVEL_DEFAULT:
			zLevel = "HiddenMapZoom";
			break;
		case ZOOMLEVEL_LOCATION:
			zLevel = "HiddenMapLocationZoom";
			break;
		case ZOOMLEVEL_COUNTRY:
			zLevel = "HiddenMapCountryZoom";
			break;
		default:
			zLevel = "HiddenMapZoom";
			break;
	}
	return parseInt($("#" + zLevel + "_" + zType).val());
}

