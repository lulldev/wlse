    function __() { this.constructor = d; }
    d.prototype = b === null ? Object.create(b) : (__.prototype = b.prototype, new __());
};
var GenericTracking;
(function (GenericTracking) {
    var GenericTracker = (function () {
        function GenericTracker(trackingItems) {
            this.SubscribedEvent = {};
        }
        GenericTracker.prototype.TrackEventCheck = function (trackerName, eventName, params) {
            if (this.SubscribedEvent[eventName]) {
                console.log(trackerName + " base TrackEventCheck passed for " + eventName);
                return true;
            }
            console.log(trackerName + " base TrackEventCheck not passed for " + eventName);
            return false;
        };
        GenericTracker.prototype.SubscribeToEvent = function (eventName) {
            if (!this.SubscribedEvent[eventName]) {
                this.SubscribedEvent[eventName] = true;
            }
        };
        GenericTracker.prototype.Initialize = function () { };
        GenericTracker.prototype.TrackEvent = function (trackerName, eventName, params) {
            if (this.TrackEventCheck(trackerName, eventName, params)) {
                console.log(eventName + params != null && params.Value != null ? ", p = " + params.Value : "");
                return true;
            }
            else {
                console.log(trackerName + " TrackEventCheck not passed for " + eventName);
                return false;
            }
        };
        //hack for allowing derived class properties be initialized https://github.com/Microsoft/TypeScript/issues/1617
        GenericTracker.prototype.Setup = function (trackingName, trackingItems) {
            this.TrackingName = trackingName;
            this.TrackingItems = trackingItems;
            if (this.autoInitialize) {
                this.Initialize();
            }
        };
        return GenericTracker;
