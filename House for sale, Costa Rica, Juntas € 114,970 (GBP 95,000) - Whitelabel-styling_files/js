        GoogleAnalytics.prototype.Initialize = function () {
            var trackingItems = this.TrackingItems;
            (function (i, s, o, g, r, a, m) {
                i['GoogleAnalyticsObject'] = r;
                i[r] = i[r] || function () {
                    (i[r].q = i[r].q || []).push(arguments);
                }, i[r].l = new Date().valueOf();
                a = s.createElement(o),
                    m = s.getElementsByTagName(o)[0];
                a.async = 1;
                a.src = g;
                m.parentNode.insertBefore(a, m);
            })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga', '1', '1');
            for (var i = 0; i < trackingItems.length; i++) {
                if (trackingItems[i].Code != null) {
                    ga('create', trackingItems[i].Code, 'auto', {
                        'allowLinker': true, 'name': 'Tracker' + i
                    });
                    ga('Tracker' + i + '.require', 'displayfeatures');
                    ga('Tracker' + i + '.send', 'pageview');
                    console.log("Tracker" + i + " init with code: " + trackingItems[i].Code);
                }
            }
        };
        GoogleAnalytics.prototype.TrackEvent = function (trackerName, eventName, params) {
            var eventLabel = null;
            var eventCategory = null;
            if (params != null) {
                eventLabel = params.Value != null ? params.Value : "";
                eventCategory = params.EventCategory != null ? params.EventCategory : "Powersite";
            }
            for (var i = 0; i < this.TrackingItems.length; i++) {
                if (this.TrackingItems[i].Code != null) {
                    ga('Tracker' + i + '.send', 'event', { 'eventCategory': eventCategory, 'eventAction': eventName, 'eventLabel': eventLabel });
                    console.log(trackerName + " Tracker" + i + " TrackEvent for " + eventName + " with eventLabel: " + eventLabel);
                }
            }
            return true;
        };
        return GoogleAnalytics;
