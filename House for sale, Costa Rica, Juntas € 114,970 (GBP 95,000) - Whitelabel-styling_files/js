            // IVW VERSION="1.2"
            var wemfImg = document.getElementById('wemfImg');
            if (wemfImg != null) {
                if ('https:' == document.location.protocol) {
                    var WEMF = "https://home-ssl.wemfbox.ch/cgi-bin/ivw-ssl/CP/ssl";
                    wemfImg.src = WEMF + '?r=' + encodeURI(document.referrer) + '&d=' + (Math.random() * 100000);
                }
                else {
                    var IVW = "http://homegate.wemfbox.ch/cgi-bin/ivw/CP/immostreet";
                    wemfImg.src = IVW + '?r=' + encodeURI(document.referrer) + '&d=' + (Math.random() * 100000);
                }
            }
            return true;
        };
        NetMetrixAudit.prototype.TrackEventCheck = function (trackerName, eventName, params) {
            if (!_super.prototype.TrackEventCheck.call(this, trackerName, eventName, params)) {
                return false;
            }
            return true;
        };
        return NetMetrixAudit;
    }(GenericTracker));
    GenericTracking.NetMetrixAudit = NetMetrixAudit;
    var PingdomTracker = (function (_super) {
        __extends(PingdomTracker, _super);
        function PingdomTracker(trackingName, trackingItems) {
            _super.call(this, trackingItems);
            this.autoInitialize = true;
            this.Setup(trackingName, trackingItems);
        }
        PingdomTracker.prototype.Initialize = function () {
            var trackingItem = this.TrackingItems[0];
