                    var s = document.getElementsByTagName('script')[0];
                    s.parentNode.insertBefore(fbds, s);
                    _fbq.loaded = true;
                }
                _fbq.push(['addPixelId', trackingItem.Code]);
            })();
            console.log("Add FacebookCustomAudience pixel with code: " + trackingItem.Code);
            window._fbq = window._fbq || [];
