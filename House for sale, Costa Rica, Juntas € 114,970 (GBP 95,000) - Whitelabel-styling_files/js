    var TrackingEvent = GenericTracking.TrackingEvent;
    var Trackers = (function () {
        function Trackers() {
            this.TrackerList = {};
        }
        Object.defineProperty(Trackers, "Instance", {
            get: function () {
                if (this.instance === null || this.instance === undefined) {
                    this.instance = new Trackers();
                }
                return this.instance;
            },
            enumerable: true,
            configurable: true
        });
        Trackers.prototype.AddGenericTracker = function (trackingConfig) {
            var tracker = null;
            var trackingModelsList = null;
            var trackingModel = null;
            if (typeof (trackingConfig) != "undefined" && trackingConfig != null) {
                trackingModelsList = typeof (trackingConfig.TrackingModel) != "undefined" ? trackingConfig.TrackingModel : null;
            }
            if (trackingModelsList != null) {
                for (var i = 0; i < trackingModelsList.length; i++) {
                    trackingModel = trackingModelsList[i];
                    var trackingName = trackingModel.TrackingName;
                    console.log("Use " + trackingName);
                    switch (trackingName) {
                        case TrackingName[TrackingName.FacebookCustomAudiencePixel]:
                            var trackingDataModel = typeof (trackingConfig.TrackingDataModel) != "undefined" ? trackingConfig.TrackingDataModel : null;
                            tracker = new GenericTracking.FacebookCustomAudiencePixel(trackingName, trackingModel.TrackingItems, trackingDataModel);
                            break;
                        case TrackingName[TrackingName.FacebookConversionPixel]:
                            tracker = new GenericTracking.FacebookConversionPixel(trackingName, trackingModel.TrackingItems);
                            break;
                        case TrackingName[TrackingName.GoogleAdWordsConvertion]:
                            tracker = new GenericTracking.GoogleAdWordsConvertion(trackingName, trackingModel.TrackingItems);
                            break;
                        case TrackingName[TrackingName.GoogleAnalytics]:
                            if (typeof (trackingConfig.GlobalCode) != "undefined" && trackingConfig.GlobalCode != null) {
                                trackingModel.TrackingItems.push({ "Code": trackingConfig.GlobalCode });
                            }
                            tracker = new GenericTracking.GoogleAnalytics(trackingName, trackingModel.TrackingItems);
                            break;
                        case TrackingName[TrackingName.Heatmap]:
                            tracker = new GenericTracking.HeatmapTracker(trackingName, trackingModel.TrackingItems);
                            break;
                        case TrackingName[TrackingName.Hotjar]:
                            tracker = new GenericTracking.HotjarTracker(trackingName, trackingModel.TrackingItems);
                            break;
                        case TrackingName[TrackingName.ListHub]:
                            tracker = new GenericTracking.ListHubTracker(trackingName, trackingModel.TrackingItems);
                            break;
                        case TrackingName[TrackingName.NetMetrixAudit]:
                            tracker = new GenericTracking.NetMetrixAudit(trackingName, trackingModel.TrackingItems);
                            break;
                        case TrackingName[TrackingName.Pingdom]:
                            tracker = new GenericTracking.PingdomTracker(trackingName, trackingModel.TrackingItems);
                            break;
                        //case TrackingName[TrackingName.GoogleTagManager]:
                        //	tracker = new GoogleTagManagerTracker(trackingName, trackingModel.TrackingItems);
                        //	break;
                        case TrackingName[TrackingName.EventTracking]:
                            tracker = new GenericTracking.EventTracker(trackingName, trackingModel.TrackingItems);
                            break;
                        default:
                            console.log(trackingName + " tracker name is not found in tracking service.");
                            break;
                    }
                    if (tracker != null) {
                        console.log("Tracker " + tracker.TrackingName + " added");
                        this.TrackerList[tracker.TrackingName] = tracker;
                        tracker = null;
                    }
                }
            }
        };
        Trackers.prototype.TrackEvent = function (eventName, params) {
            for (var name in this.TrackerList) {
                if (this.TrackerList[name].TrackEventCheck(name, eventName, params)) {
                    this.TrackerList[name].TrackEvent(name, eventName, params);
                }
                else {
                    console.log(name + " TrackEventCheck not passed for " + eventName);
                }
            }
        };
        Trackers.prototype.SubscribeToEvent = function (eventName, trackingName) {
            if (this.TrackerList[trackingName]) {
                this.TrackerList[trackingName].SubscribeToEvent(eventName);
            }
        };
        return Trackers;
    }());
    GenericTracking.Trackers = Trackers;
})(GenericTracking || (GenericTracking = {}));

